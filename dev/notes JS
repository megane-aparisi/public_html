/* Notes de rappel : */
var nombretest = 1;
let nombre2 = 2; /*La var va changer. la var est accessible uniquement à la ligne déclarée*/
const nombres = 3; /*la var est constante*/
document.write(nombretest); /*ca sert à écrire dans le html*/
nombretest = "nombre";
document.write("<br>");
document.write(nombretest);


